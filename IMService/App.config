<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/>
    </startup>
  <connectionStrings>
    <!--数据库连接字符串，注：请勿删除和更改name
    -->
    <add name="KylinIMConnectionString" connectionString="server=192.168.1.200;user id=kylinim;password=123456;database=kylinim"/>
  </connectionStrings>
  <appSettings>
    <!--数据库类型，mssql|pgsql-->
    <add key="SqlType" value="mssql"/>
    <!--IM服务器信息
    *
    -->
    <add key="Host" value="192.168.1.139"/>
    <add key="Port" value="8088"/>
    <add key="MaxConnectionCount" value="100"/>
  </appSettings>
</configuration>
